          <input type="number" id="userId" class="form-control" required>
        </div>
        <button class="btn btn-primary" id="retrieveUserInfo">Retrieve</button>
      <!-- </form> -->
    </div>
    <div class="container pt-3" id="userinfo">
    
    </div>
    
    <div class="container pt-3" id="gifResults"></div>
    <div class="container pt-3">
      <h2 class="display-5">Chat Area</h2>
